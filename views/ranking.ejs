                    </button>
                    <a class="navbar-brand" href="#">
                         <strong>Proyecto DSI</strong>
                    </a>
                    <div class="collapse navbar-collapse" id="navbarNav1">
                        <ul class="navbar-nav mr-auto">
                            <li class="nav-item active">
                                 <a class="nav-link" href="/">Home </a>
                            </li>
                            <li class="nav-item">
                                 <a class="nav-link" href="/ranking">Ranking</a>
                            </li>
                             <li class="nav-item">
                                 <a class="nav-link" href="/profile">Perfil</a>
                            </li>
                        </ul>
                        <form class="form-inline waves-effect waves-light">
                            <input class="form-control" type="text" placeholder="Search">
                        </form>
                    </div>
                </div>
            </nav>
            <!--/.Navbar-->
        </header>
        <main>
            <!--Main layout-->
            <div class="container">
                <!--Page heading-->
                <div class="row">
                    <div class="offset-1 col-md-11">
                       
                         <h1>Ranking 3nWin</h1>
                 <ul class="users">
                        <table>
                            <thead>
                                <tr>
                                    <th data-priority="1">Posición</th>
                                    <th data-priority="1">Nombre</th>
                                    <th data-priority="2">Totales</th>
                                    <th data-priority="4">Ganadas</th>
                                    <th data-priority="3">Perdidas</th>
                                    <th data-priority="5">Empatadas</th>
                                    <th data-priority="6">% Ganadas</th>
                                </tr>
                            </thead>
                            <tbody> 
                                   <% for(var i=0; i < users.length; i++) {%>
                                    <li class="users">
                                    <tr>
                                      <td data-priority="1"><%= i+1 %> </td>
                                      <td data-priority="2"><%= users[i].local.name%> </td>

                                      <td data-priority="2"><%= users[i].local.totales%> </td>
                                      <td data-priority="3"><%= users[i].local.ganadas %></td>
                                      <td data-priority="4"><%=  users[i].local.perdidas %></td>
                                      <td data-priority="5"><%=  users[i].local.empatadas %></td>
                                      <td data-priority="6"><%=  ((users[i].local.ganadas/users[i].local.totales)*100).toFixed(2) %>%</td>
                                     </tr>    </li>
                                  <% } %>
                                  </ul>
                            </tbody>
                        </table>

                    </div>
                </div>
            </div>

            <!--/.Main layout-->
        </main>

