        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta.2/css/bootstrap.min.css" integrity="sha384-PsH8R72JQ3SOdhVi3uxftmaW6Vc51MKb0q5P2rRUpPvrszuE4W1povHYgTpBfshb" crossorigin="anonymous">

        <link href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.css" rel="stylesheet">
        <link href="css/style.css" rel="stylesheet">



        <style>
        
            .fade {
                opacity:0.5;
            }
            .fade:hover {
                opacity:1;
            }
            .carousel-inner {
                height: 515px;
            }
            .carousel-inner img {
                width: 100%;
                height: 470px;
            }
        </style>
    </head>


    <body>

        <header>

            <!--Navbar-->
            <nav class="navbar navbar-toggleable-md navbar-dark">
                <div class="container">
                    <button class="navbar-toggler navbar-toggler-right" type="button" data-toggle="collapse" data-target="#navbarNav1" aria-controls="navbarNav1" aria-expanded="false" aria-label="Toggle navigation">
                         <span class="navbar-toggler-icon"></span>
                    </button>
                    <a class="navbar-brand" href="#">
                         <strong>Proyecto SYTW</strong>
                    </a>
                </div>
            </nav>
            <!--/.Navbar-->
           
            
            
        </header>
        <main>
            <!--Main layout-->
            <div class="container">

                <!--Page heading-->
                <div class="row">
                    <div id="carouselExampleIndicators" class="carousel slide col-md-4" data-interval="2000" data-ride="carousel">
                        <ol class="carousel-indicators">
                            <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
                            <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
                            <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
                             <li data-target="#carouselExampleIndicators" data-slide-to="3"></li>
                        </ol>
                        <div class="carousel-inner" role="listbox">
                            <div class="carousel-item active">
                                <img class="d-block img-fluid" src="img/3_en_rayas_violeta.png" alt="First slide">
                            </div>
                            <div class="carousel-item">
                                <img class="d-block img-fluid" src="img/ajedrez_violeta.png" alt="Second slide">
                            </div>
                            <div class="carousel-item">
                                <img class="d-block img-fluid" src="img/buscaminas_violetas.png" alt="Third slide">
                            </div>
                             <div class="carousel-item">
                                <img class="d-block img-fluid" src="img/damas_violetas.png" alt="Third slide">
                            </div>
                        </div>
                        <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
                             <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                             <span class="sr-only">Previous</span>
                        </a>
                        <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
                             <span class="carousel-control-next-icon" aria-hidden="true"></span>
                             <span class="sr-only">Next</span>
                        </a>
                    </div>


                    <div class="col-md-8 ng-scope ng-building" ng-controller="controladorConMetodos">
                        <div class="omb_login">
                            <div>
                                <div>
                                     <h3 class="omb_authTitle"><a  href="" ng-click="ShowRetroceder()" >
                                         Login</a> or <a href=""  ng-click="ShowForm()">Sign up</a></h3>
                                </div>

                                <div class="row omb_row-sm-offset-3 omb_socialButtons" ng-controller="verTextRS">


                                    <div class="col-xs-3 col-sm-3" style={}>
                                        <a href="/auth/facebook" class="btn btn-lg btn-block omb_btn-facebook" onclick="window.location='/auth/facebook';">
                                             <i class="fa fa-facebook visible-xs"></i>
                                            <br>Facebook

                                        </a>
                                    </div>
                                    <div class="col-xs-3 col-sm-3">
                                        <a href="/auth/twitter" class="btn btn-lg btn-block omb_btn-twitter" onclick="window.location='/login/twitter';">
                                             <i class="fa fa-twitter visible-xs"></i>
                                            <br>Twitter
                                        </a>
                                    </div>
                                    <div class="col-xs-3 col-sm-3">
                                        <a href="/login/github" class="btn btn-lg btn-block omb_btn-github" onclick="window.location='/login/github';">
                                             <i class="fa fa-github visible-xs" ng-model="Github"></i>
                                            <br>{{Github}}
                                        </a>
                                    </div>

                                </div>

                                <div ng-hide="form">
                                    <div class="row omb_row-sm-offset-3 omb_loginOr">
                                        <div class="col-xs-12 col-sm-9">
                                            <hr class="omb_hrOr">
                                            	<span class="omb_spanOr">or</span>
                                        </div>
                                    </div>


                                    <div class="row omb_row-sm-offset-3">
                                     
                                      <!--div class="col-xs-12 col-sm-9" ng-controller="loginCtrl as vm"-->
<div class="col-xs-12 col-sm-9">
                                            <!--form class="omb_loginForm" method="POST" action="/login" method="post ng-submit="vm.enviar()"-->
                                       <form class="omb_loginForm" method="POST" action="/login">
                                                <div class="input-group">
                                                    	<span class="input-group-addon"><i class="fa fa-user"></i></span>
                                                    <input required type="text" ng-model="vm.fdatos.username" class="form-control" name="username" placeholder="Nombre Usuario">
                                                </div>
                                                 <span class="help-block"></span>

                                                <div class="input-group">
                                                    	<span class="input-group-addon"><i class="fa fa-lock"></i></span>
                                                    <input required type="password" class="form-control" ng-model="vm.fdatos.password" name="password" placeholder="Password">
                                                </div>
                                                <div>
                                                     <a type="text" class="link" href="" ng-click="ShowForm()">Ya tienes cuenta? Regístrate aquí</a>

                                                </div>
                                                <button class="btn btn-lg btn-primary btn-block" value="Log in" type="submit">Login</button>
                                                <!---a class="link" href="/registro">No tienes cuenta? Regístrate aquí</a-->



                                            </form>




                                        </div>

                                    </div>

                                </div>


                            </div>
                            <!--p ng-controller="dentro">Nombre: {{vm.fdatos.nombre}} Contraseña:{{vm.fdatos.password}} ContraseOtra:{{vm.fdatos.passwordh}}</p>
                            <!--Login-->

                            <div>
                                <div class="omb_login" ng-style="estilos" style="display: none;" ng-show="form">

                                    <div class="row omb_row-sm-offset-3 omb_loginOr">
                                        <div class="col-xs-12 col-sm-9">
                                            <hr class="omb_hrOr"><span class="omb_spanOr">or</span>	
                                        </div>
                                    </div>
                                    
                                    <div class="row omb_row-sm-offset-3">
                                        <div class="col-xs-12 col-sm-9" ng-controller="registroCtrl as rg" >
                                            <form class="omb_loginForm"  ng-submit="rg.registrar()">
                                                <div class="input-group">
                                                    	<span class="input-group-addon"><i class="fa fa-user"></i></span> 

                                                    <input type="text" class="form-control" ng-model="rg.registro.username" name="username" id="username" placeholder="Introduce tu nombre de usuario">
                                                </div>

                                                	<span class="help-block"></span>
                                                <div class="input-group">
                                                    	<span class="input-group-addon"><i class="fa fa-lock"></i></span>
                                                    <input type="password" class="form-control" ng-model="rg.registro.password" name="password" placeholder="Password" id="password">
                                                </div>
                                                <div class="input-group">
                                                    	<span class="input-group-addon"><i class="fa fa-lock"></i></span>
              <input type="password" class="form-control" ng-model="rg.registro.passwordh" name="passwordh" placeholder="Repite tu password" id="Password2">
                                                </div>



                                                 <a class="link" href="" ng-click="ShowRetroceder()">Ya tienes cuenta? Ir a login</a>


                                                <button class="btn btn-lg btn-primary btn-block" value="Log in" type="submit">Registrarse</button>
                                                <!--<button class="btn btn-lg btn-primary btn-block" type="submit"><a class="link" href="/registro">Sign Up </a></button>-->
                                                <!--a class="link" href="/">Tienes cuenta? Pincha aquí</a-->
                                            </form>



                                            <div>







                                                
<a class="link" href="/cambiarpass">Cambiar contraseña hacerlos?</a>
                                            </div>

                                        </div>






                                    </div>


                                    <!--p ng-controller="dentro">Nombre: {{registro.username}} Contraseña:{{registro.password}} ContraseOtra:{{registro.passwordh}}</p>

                                    <!--./Login-->





                                    <div class="row omb_row-sm-offset-3">
                                        <div class="col-xs-12 col-sm-3">

                                        </div>
                                        <div class="col-xs-12 col-sm-3">
                                            <p class="omb_forgotPwd">
                                                <!--<a href="#">Forgot password?</a>-->
                                            </p>
                                        </div>
                                    </div>
                                </div>


                            </div>
                        </div>
                    </div>


                </div>
            </div>
            <!--/.Main layout-->
          
        </main>

        <!--Footer-->
        <footer class="page-footer center-on-small-only">

            <div ng-controller="PieCtrl">
                <div ng-include src="footer.url"></div>
            </div>

        </footer>
